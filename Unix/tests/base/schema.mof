};

class Inner
{
    [Key] Uint32 Key;
    String Str;
};

class Outer
{
    [Key] Uint32 Key;

    // Scalar types:
    Boolean booleanScalar;
    Uint8 uint8Scalar;
    Sint8 sint8Scalar;
    Uint16 uint16Scalar;
    Sint16 sint16Scalar;
    Uint32 uint32Scalar;
    Sint32 sint32Scalar;
    Uint64 uint64Scalar;
    Sint64 sint64Scalar;
    Real32 real32Scalar;
    Real64 real64Scalar;
    Char16 char16Scalar;
    Datetime datetimeScalar;
    String stringScalar;
    [EmbeddedInstance("Inner")]
    String instanceScalar;

    // Array types:
    Boolean booleanArray[];
    Uint8 uint8Array[];
    Sint8 sint8Array[];
    Uint16 uint16Array[];
    Sint16 sint16Array[];
    Uint32 uint32Array[];
    Sint32 sint32Array[];
    Uint64 uint64Array[];
    Sint64 sint64Array[];
    Real32 real32Array[];
    Real64 real64Array[];
    Char16 char16Array[];
    Datetime datetimeArray[];
    String stringArray[];
    [EmbeddedInstance("Inner")] 
    String instanceArray[];
};
